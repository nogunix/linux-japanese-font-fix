!_TAG_ROLE_DESCRIPTION!Make!makefile	included	/included/
!_TAG_ROLE_DESCRIPTION!Make!makefile	optional	/optionally included/
!_TAG_ROLE_DESCRIPTION!Yaml!anchor	alias	/alias/
COPR_DIR	Makefile	/^COPR_DIR := $(abspath $(dir $(lastword $(MAKEFILE_LIST))))$/;"	language:Make
OUTDIR	Makefile	/^OUTDIR := $(COPR_DIR)$/;"	language:Make
OUTDIR	Makefile	/^OUTDIR := $(abspath $(outdir))$/;"	language:Make
REPO_ROOT	Makefile	/^REPO_ROOT := $(abspath $(COPR_DIR)\/..)$/;"	language:Make
SOURCES	Makefile	/^SOURCES := $(TOPDIR)\/SOURCES$/;"	language:Make
SOURCE_FILES	Makefile	/^SOURCE_FILES := $(addprefix $(SPEC_DIR)\/,$(TARBALLS))$/;"	language:Make
SPEC_DIR	Makefile	/^SPEC_DIR := $(REPO_ROOT)$/;"	language:Make
SPEC_DIR	Makefile	/^SPEC_DIR := $(abspath $(spec))$/;"	language:Make
SPEC_FILE	Makefile	/^SPEC_FILE := $(abspath $(SPEC_DIR)\/linux-japanese-font-fix.spec)$/;"	language:Make
SRPMS	Makefile	/^SRPMS := $(TOPDIR)\/SRPMS$/;"	language:Make
TARBALLS	Makefile	/^TARBALLS := 50-user-jp-fonts.conf README.md README.ja.md LICENSE$/;"	language:Make
TOPDIR	Makefile	/^TOPDIR := $(COPR_DIR)\/_build$/;"	language:Make
srpm	Makefile	/^srpm:$/;"	language:Make
srpm_path	Makefile	/^srpm_path:$/;"	language:Make
